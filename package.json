{
	"name": "elm",
	"description": "Syntax highlighting for elm",
	"version": "0.0.3",
	"publisher": "sbrink",
	"author": {
		"name": "Sascha Brink"
	},
    "icon": "images/elmIcon.png",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/sbrink/vscode-elm.git"
	},
	"contributes": {
		"languages": [{
			"id": "elm",
			"aliases": ["Elm", "elm"],
			"extensions": [".elm"],
			"configuration": "./elm.configuration.json"
		}],
		"grammars": [{
			"language": "elm",
			"scopeName": "source.elm",
			"path": "./syntaxes/elm.json"
		}],
		"snippets": [{
			"language": "elm",
			"path": "./snippets/elm.json"
		}]
	}
}